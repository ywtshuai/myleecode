class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int n = size(nums);
vector<int> result(2);
for (int i = 0; i < n-1; i++)
{
	for (int j = i+1; j < n; j++)
	{
		if (nums[j] == target - nums[i])
         {
			result[0] = i;
			result[1] = j;
			return result;
		}
	
	}
}
return result;
} 
};
